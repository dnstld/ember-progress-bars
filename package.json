{
  "name": "ember-progress-bars",
  "version": "1.0.0-beta-08",
  "description": "A simple Ember.js progress bar",
  "keywords": [
    "progress",
    "progress bar",
    "progress bars",
    "ui element",
    "ui elements",
    "ember-addon"
  ],
  "repository": "https://github.com/dnstld/ember-progress-bars",
  "license": "MIT",
  "author": "Denis Toledo <hi@denistoledo.dev>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:styles": "stylelint app/styles/**/*.scss",
    "lint:js": "eslint .",
    "lint:all": "concurrently 'npm run lint:js -- --quiet' 'npm run lint:hbs' 'npm run lint:styles'",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "husky": {
